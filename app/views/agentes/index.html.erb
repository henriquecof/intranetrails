<p id="notice"><%= notice %></p>

<h1>Agentes</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Fone</th>
      <th>Endereco</th>
      <th>Uf</th>
      <th>Cidade</th>
      <th>Tipo</th>
      <th>Cpfcnpj</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agentes.each do |agente| %>
      <tr>
        <td><%= agente.nome %></td>
        <td><%= agente.fone %></td>
        <td><%= agente.endereco %></td>
        <td><%= agente.uf %></td>
        <td><%= agente.cidade %></td>
        <td><%= agente.tipo %></td>
        <td><%= agente.cpfcnpj %></td>
        <td><%= agente.email %></td>
        <td><%= link_to 'Show', agente %></td>
        <td><%= link_to 'Edit', edit_agente_path(agente) %></td>
        <td><%= link_to 'Destroy', agente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agente', new_agente_path %>
